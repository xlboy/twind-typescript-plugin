{
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "port": 6666,
      "sourceMaps": true,
      "name": "Attach to VS Code TS Server via Port",
      "processId": "${command:PickProcess}",
      "sourceMapRenames": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}